 '(epg-gpg-program "/usr/local/bin/gpg")
 '(org-agenda-files
   '("~/org/organizer.org" "~/org/clients/Bottles.org" "~/org/clients/JexReality.org" "~/org/clients/WheelWays.org" "~/org/schedule.org" "/Users/djruesch/org/journal/2020-12-29.org" "/Users/djruesch/org/journal/2020-12-28.org"))
 '(org-journal-date-format "%e %b %Y - (%A)")
 '(org-journal-date-prefix "* ")
 '(org-journal-dir "~/org/journals/")
